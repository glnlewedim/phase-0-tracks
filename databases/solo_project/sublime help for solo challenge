CREATE TABLE Party_Affiliation (
  house_or_senate_id INTEGER PRIMARY KEY,
  Affiliation VARCHAR (255),
  Visibility BOOLEAN
);

CREATE TABLE Politician (
	id integer primary key,
  	name VARCHAR(255),
  	age INT,
    house_or_senate_id INT,
  FOREIGN KEY (house_or_senate_id) REFERENCES Party_Affilication(house_or_senate_id)
  
);

